{
  "extends": "./tsconfig",
  "compilerOptions": {
    "module": "preserve",
    "outDir": "./dist/",
    "allowJs": false,
    "noEmit": false,
    "jsx": "react-jsx",
    "emitDeclarationOnly": false,
    "moduleResolution": "bundler",
    "skipLibCheck": true,
    "declaration": true
  },
  "include": [
    "@types/**/*.ts",
    "./src/export/index.tsx",
  ],
  "exclude": []
}
